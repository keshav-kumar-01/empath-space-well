{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://chetna.life/$1",
      "has": [
        {
          "type": "host",
          "value": "www.chetna.life"
        }
      ],
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
